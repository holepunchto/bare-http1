{
  "name": "bare-http1",
  "version": "4.0.4",
  "description": "Native HTTP/1 library for JavaScript",
  "exports": {
    "./package": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./constants": {
      "types": "./lib/constants.d.ts",
      "default": "./lib/constants.js"
    },
    "./errors": {
      "types": "./lib/errors.d.ts",
      "default": "./lib/errors.js"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "lib"
  ],
  "scripts": {
    "test": "prettier . --check && bare test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-http1.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-http1/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-http1#readme",
  "dependencies": {
    "bare-events": "^2.6.0",
    "bare-stream": "^2.3.0",
    "bare-tcp": "^2.0.3"
  },
  "devDependencies": {
    "bare-buffer": "^3.0.2",
    "bare-url": "^2.1.3",
    "brittle": "^3.3.0",
    "prettier": "^3.4.1",
    "prettier-config-standard": "^7.0.0"
  },
  "peerDependencies": {
    "bare-buffer": "*",
    "bare-url": "*"
  },
  "peerDependenciesMeta": {
    "bare-buffer": {
      "optional": true
    },
    "bare-url": {
      "optional": true
    }
  }
}
